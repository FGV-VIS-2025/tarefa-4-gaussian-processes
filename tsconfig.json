{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "baseUrl": ".",
	  "paths": {
		"$lib": ["src"],
		"$lib/*": ["src/*"],
		"$actions/*": ["src/actions/*"],
		"$components/*": ["src/components/*"],
		"$data/*": ["src/data/*"],
		"$stores/*": ["src/stores/*"],
		"$styles/*": ["src/styles/*"],
		"$svg/*": ["src/svg/*"],
		"$utils/*": ["src/utils/*"]
	  },
	  "verbatimModuleSyntax": true,
	  "allowJs": true,
	  "checkJs": false
	},
	"include": ["src/**/*.js",
		"src/**/*.svelte"
	]
  }
  